        <div class="search-options">
            <p>Discover More</p>

            <ul class="search-options-list">
                <% navbar.searchbar.forEach(options => { %>
                    <li class="search-options-list-item">
                        <a href="#">
                            <div>
                                <img width="32px" height="32px" loading="lazy" class="_396cs4 _1uwInh" alt="" src="https://rukminim1.flixcart.com/www/100/100/promos/20/07/2022/ea2077a7-575d-4fb9-a4dd-6df16e53c5d8.png?q=90">
